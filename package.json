{
  "name": "finger-haptic-feedback-system",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "指套触感反馈系统 - 基于YOLO11物体检测和3D交互的多端Web应用",
  "keywords": [
    "haptic-feedback",
    "yolo11",
    "3d-interaction",
    "ar",
    "react",
    "typescript"
  ],
  "author": "Ymeta Team",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:prod": "tsc -b && vite build --config vite.config.prod.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "check": "tsc -b --noEmit",
    "serve": "node serve.cjs",
    "start": "npm run serve",
    "docker:build": "docker build -t finger-haptic-feedback:latest .",
    "docker:run": "docker run -p 8080:80 finger-haptic-feedback:latest",
    "docker:compose": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "deploy:local": "node -e \"require('child_process').spawn(process.platform === 'win32' ? 'powershell' : 'bash', [process.platform === 'win32' ? './deploy.ps1' : './deploy.sh', 'local'], {stdio: 'inherit'})\"",
    "deploy:build-only": "node -e \"require('child_process').spawn(process.platform === 'win32' ? 'powershell' : 'bash', [process.platform === 'win32' ? './deploy.ps1' : './deploy.sh', 'local', process.platform === 'win32' ? '-BuildOnly' : '--build-only'], {stdio: 'inherit'})\""
  },
  "dependencies": {
    "@mediapipe/camera_utils": "^0.3.1675466862",
    "@mediapipe/drawing_utils": "^0.3.1675466124",
    "@mediapipe/hands": "^0.4.1675469240",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-switch": "^1.2.6",
    "@react-three/drei": "^9.122.0",
    "@react-three/fiber": "^8.18.0",
    "@tensorflow/tfjs": "^4.22.0",
    "@tensorflow/tfjs-backend-webgl": "^4.22.0",
    "@types/matter-js": "^0.20.2",
    "@types/three": "^0.180.0",
    "clsx": "^2.1.1",
    "express": "^4.21.2",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.511.0",
    "matter-js": "^0.20.0",
    "onnxruntime-web": "^1.23.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-router-dom": "^7.3.0",
    "tailwind-merge": "^3.0.2",
    "three": "^0.180.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/node": "^22.15.30",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.21",
    "babel-plugin-react-dev-locator": "^1.0.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "terser": "^5.44.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vite-plugin-trae-solo-badge": "^1.0.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
